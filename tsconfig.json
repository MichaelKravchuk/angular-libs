{
  "compileOnSave": false,
  "angularCompilerOptions": {
    "enableIvy": false
  },
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./docs/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@shared/*": [
        "src/app/shared/*"
      ],
      "angular-resize-element": [
        "dist/angular-resize-element"
      ],
      "angular-resize-element/*": [
        "dist/angular-resize-element/*"
      ],
      "angular-move-element": [
        "dist/angular-move-element"
      ],
      "angular-move-element/*": [
        "dist/angular-move-element/*"
      ],
      "ct-dynamic-form": [
        "dist/ct-dynamic-form"
      ],
      "ct-dynamic-form/*": [
        "dist/ct-dynamic-form/*"
      ],
      "spring-slider": [
        "dist/spring-slider/spring-slider",
        "dist/spring-slider"
      ]
    }
  }
}